{
  "openapi" : "3.0.0",
  "info" : {
    "title" : "Bisnode Common Components",
    "description" : "Common Date models",
    "version" : "1.0.0"
  },
  "components" : {
    "schemas" : {
      "Date" : {
        "type" : "object",
        "description" : "Date object, split up in year, month and day. Null values will be omitted.\nThis model is used in cases where we don't always have a full date.\n",
        "example" : "'{\n  \"year\": \"2012\",\n  \"month\": \"1\",\n  \"day\": \"23\"\n}'\n",
        "properties" : {
          "year" : {
            "type" : "integer",
            "format" : "int32",
            "description" : "Full year including century",
            "example" : 2012
          },
          "month" : {
            "type" : "integer",
            "format" : "int32",
            "description" : "Month-number",
            "minimum" : 1,
            "maximum" : 12,
            "example" : 2
          },
          "day" : {
            "type" : "integer",
            "format" : "int32",
            "minimum" : 1,
            "maximum" : 31,
            "description" : "Day of month",
            "example" : 23
          }
        },
        "required" : [ ],
        "title" : "Date"
      }
    }
  }
}